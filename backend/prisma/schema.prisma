generator client {
  provider = "prisma-client-js"
}

// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room {
  id            String  @id
  name          String
  password      String
  players       Json
  gameState     Json?
  prevGameState Json?
  ownerId       String?
  version       Int     @default(0)
}
